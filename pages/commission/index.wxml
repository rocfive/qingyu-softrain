<!--pages/commission/index.wxml-->
<view class='head'>
  <image src='../img/doctor.png' class='headpic'></image>
  <view class='head_m'>
    <view class='name'>用户昵称</view>
    <view class='btn'>普通等级</view>
  </view>
  <view class='head_r'>
    <view class='money'>￥24359.00</view>
    <view>总销售额</view>
  </view>
</view>
<!-- <view class='yqm'>我的邀请码：2156</view> -->
<view class='yongj'>
  <view class='money'>￥1.00</view>
  <view>成功提现佣金(元）</view>
</view>
<view class='yongj' style='margin-top:0'>
  <view class='money'>￥1.00</view>
  <view>可提现佣金(元）</view>
  <view class='tx_btn' bindtap='tx' wx:if='{{msg.tixian_status==1}}'>佣金提现</view>
  <view class='tx_btn' bindtap='tx'>佣金提现</view>
</view>
<view class='menu'>
  <navigator url='daily' class='menu_item'>
    <text class='iconfont icon-shouru'></text>
    <text>今日收入</text>
    <text class='num'>0.00元</text>
  </navigator>
  <navigator url='money' class='menu_item'>
    <text class='iconfont icon-yongjin'></text>
    <text>分销佣金</text>
    <text class='num'>0.00元</text>
  </navigator>
  <navigator url='order' class='menu_item'>
    <text class='iconfont icon-dingdan1'></text>
    <text>分销订单</text>
    <text class='num'>0.00元</text>
  </navigator>
  <navigator url='tx_record' class='menu_item'>
    <text class='iconfont icon-tixianmingxi'></text>
    <text>提现明细</text>
    <text class='num'>0.00元</text>
  </navigator>
  <navigator url='team' class='menu_item'>
    <text class='iconfont icon-wodetuandui-copy'></text>
    <text>我的团队</text>
  </navigator>
  <navigator url='../myzoe/invite' class='menu_item'>
    <text class='iconfont icon-erweima'></text>
    <text>分享海报</text>
  </navigator>
</view>
<view class='fixed'>
  <view class='link active'>
    <text class='iconfont icon-shouye-copy-copy'></text>
    <text>分销中心</text>
  </view>
  <navigator url='money' class='link' hover-class="none">
    <text class='iconfont icon-yongjin'></text>
    <text>分销佣金</text>
  </navigator>
  <navigator url='order' class='link' hover-class="none">
    <text class='iconfont icon-dingdan1'></text>
    <text>分销订单</text>
  </navigator>
  <navigator url='team' class='link'  hover-class="none">
    <text class='iconfont icon-i'></text>
    <text>我的团队</text>
  </navigator>
</view>
