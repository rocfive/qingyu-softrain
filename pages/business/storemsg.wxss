/* pages/business/storemsg.wxss */
page{
  padding-bottom: 100rpx;
}
label{
  color: #393939
}
.item{
  height: 106rpx;
  border-bottom: 2rpx solid #f2f2f2
}
.item input{
  width: 554rpx;
  height: 106rpx;
  line-height: 106rpx;
  text-align: right
}
.item .timebox{
  width: 152rpx;
  height: 48rpx;
  border: 1rpx solid #efefef;
  text-align: center;
  line-height: 48rpx;
}
.item .item_r{
  width: 554rpx;
  height: 106rpx;
  line-height: 106rpx;
  text-align: right
}
.intro {
  padding-bottom: 20rpx;
}
.intro label{
  height: 104rpx;
  line-height: 104rpx;
}
.intro .textarea{
  width: 694rpx;
  height: 215rpx;
}
.upimg{
  width: 200rpx;
  height: 200rpx;
  background: #e9e9e9
}
.upimg .iconfont{
  color: #fff;
  font-size:72rpx;
}
.doorimg{
  width: 200rpx;
  height: 200rpx;
}
.upimg1{
  width: 150rpx;
  height: 150rpx;
  background: #efefef;
  border: 1rpx solid #bababa;
  box-sizing: border-box
}
.upimg1 .iconfont{
  font-size: 80rpx;
  color: #bababa
}
.storelist .pic{
  width: 150rpx;
  height: 150rpx;
  float: left;
  margin-right: 15rpx;
  margin-bottom: 15rpx
}
.storelist .pic:nth-child(4n){
  margin-right: 0
}
.title{
  font-size: 30rpx;
  font-weight: bold
}
.btn{
  width: 702rpx;
  height: 90rpx;
  background: linear-gradient(90deg,#3c9aff,#9e95fb);
  border-radius: 45rpx;
  text-align: center;
  line-height: 90rpx;
  color: #fff;
  font-size: 32rpx;
  margin:120rpx auto 0;
}